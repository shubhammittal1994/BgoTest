<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Change corner radius shapeable Image view -->
    <style name="CornerCut" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">cut</item>
        <item name="cornerFamilyBottomLeft">cut</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeTopLeft">20%</item>
        <item name="cornerSizeTopRight">0%</item>
        <item name="cornerSizeBottomLeft">0%</item>
        <item name="cornerSizeBottomRight">20%</item>
    </style>

    <style name="CustomTextAppearanceTab" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>